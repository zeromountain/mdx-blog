{
  "semi": true,
  "trailingComma": "all",
  "singleQuote": true,
  "printWidth": 120,
  "tabWidth": 2,
  "plugins": ["@trivago/prettier-plugin-sort-imports", "prettier-plugin-tailwindcss"],
  "importOrder": [
    "^react$",
    "^next(/.*)?$",
    "^@nextui-org/.*",
    "^(third-party|@notionhq/client|@notion-render/.*)",
    "^@/(.*)$",
    "^((./|../).*)$"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
